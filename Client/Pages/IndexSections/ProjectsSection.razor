<SectionBase Id=@Id BGLighter=@BGLighter Header="Projects" EndHref="Projects">
	<MudGrid>
		<MudItem xs="12">
			<ProjectSubSection Title="NPMS" SubPage="NPMS" Catchline="National Production Monitoring System for Nigeria" Tags="@(new() {".NET", "C#", "WinForms", "Oracle", "SQL"})">
				System to collect and monitor oil production data across Nigeria.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Anaconda" SubPage="Anaconda" Catchline="Data Collection and Presentation System" Tags="@(new() {".NET", "C#", "WPF", "MAUI", "SQL", "WCF", "C"})">
				System for the collection and presentation of a variety of data types.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Personal Portfolio Website" SubPage="Portfolio" Catchline="This Website" Tags="@(new() {"Blazor", "Azure", "C#"})">
				A website to showcase my projects and skills.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Advent Of Code 2022" SubPage="AoC" Catchline="Puzzles and Challenges" Tags="@(new() {"C#"})">
				Annual challenges involving large data and optimisation techniques.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Handover Pack Compiler" SubPage="HandoverPacks" Catchline="File Collation Tool" Tags="@(new() {"Python"})">
				A console script to augment repetitive file collation.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Neural Networks for Categorising Nanoparticles " SubPage="Nanoparticles" Catchline="Final Year Project" Tags="@(new() {"Python", "Keras", "TensorFlow"})">
				A study into neural networks and their capacity to identify and categorise STEM images.
			</ProjectSubSection>
		</MudItem>
		<MudItem xs="12">
			<ProjectSubSection Title="Swinging Nao Robot" SubPage="NaoRobot" Catchline="Group Studyx" Tags="@(new() {"Python", "Neural Networks"})">
				A group study into a robot’s swinging capacity. Using Neural Networks to find optimal solutions.
			</ProjectSubSection>
		</MudItem>
	</MudGrid>
</SectionBase>
@code {
	[Parameter]
	public string? Id { get; set; }
	[Parameter]
	public bool BGLighter { get; set; }
}
